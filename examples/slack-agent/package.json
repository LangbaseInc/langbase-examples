		"@radix-ui/react-tooltip": "^1.2.7",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.4",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
