{
	"name": "agent-app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev:vite": "vite",
		"dev:node": "cross-env NODE_ENV=development tsx watch api/index.node.ts",
		"dev:worker": "wrangler dev api/index.ts",
		"dev": "concurrently \"npm run dev:vite\" \"npm run dev:worker\"",
		"build:vite": "vite build",
		"build:worker": "wrangler deploy --dry-run api/index.ts",
		"build": "npm run build:vite && npm run build:worker",
		"deploy": "wrangler deploy api/index.ts",
		"secrets:set": "npm run secrets:langbase && npm run secrets:openai && npm run secrets:slack",
		"secrets:langbase": "wrangler secret put LANGBASE_API_KEY",
		"secrets:openai": "wrangler secret put OPENAI_API_KEY",
		"secrets:slack": "wrangler secret put SLACK_BOT_TOKEN"
	},
	"dependencies": {
		"@assistant-ui/react": "^0.10.24",
		"@assistant-ui/react-markdown": "^0.10.5",
		"@hookform/resolvers": "^5.0.1",
